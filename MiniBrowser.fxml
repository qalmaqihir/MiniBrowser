<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MiniBrowser.MiniBrowserController">
    <top>
        <ToolBar prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #9ba8ff;" BorderPane.alignment="CENTER">
            <items>
                <Button fx:id="prevButton" disable="true" mnemonicParsing="false" onAction="#prevButtonClicked" style="-fx-background-color: #000000;" text="&lt;" textFill="WHITE" />
                <Button fx:id="nextButton" disable="true" mnemonicParsing="false" onAction="#nextButtonClicked" style="-fx-background-color: #000000;" text="&gt;" textFill="WHITE" />
                <Button mnemonicParsing="false" onAction="#browse" style="-fx-background-color: #000000;" text="Browse" textFill="WHITE" />
                <TextField fx:id="addressBar" prefHeight="25.0" prefWidth="627.0" promptText="e.g. youtube.com" />
            </items>
        </ToolBar>
    </top>
    <center>
        <WebView fx:id="web" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
    </center>
</BorderPane>